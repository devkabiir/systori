                          <OutlTxt>
                            <TextOutlTxt>
                              <p>
                                <span style="font-size:11pt;background:#FFFFFF;">Elastomerbitumen-Kaltselbstklebebahn als erste Abdichtungslage</span>
                              </p>
                            </TextOutlTxt>
                          </OutlTxt>
                        </OutlineText>
                      </CompleteText>
